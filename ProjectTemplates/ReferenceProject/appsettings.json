{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "%AppData%/Logs/ReferenceProject.log",
                    "rollingInterval": "Day",
                    "buffered": false
                }
            },
            {
                "Name": "Console"
            }
        ]
    },
    "AllowedHosts": "*",
    "CoolServiceEndpoint": "http://%ORIGIN%/cool",
    "AnotherServiceEndpoint": "http://%ORIGIN%/another"
}
